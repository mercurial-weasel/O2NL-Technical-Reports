    "path": "^0.12.7",
    "pg": "^8.14.1",
    "plotly": "^1.0.6",
    "plotly.js-dist": "^3.0.1",
    "plotly.js-dist-min": "^2.35.3",
    "proj4": "^2.15.0",
    "react": "^18.3.1",
    "react-datepicker": "^6.9.0",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^6.28.2",
    "react-slider": "^2.0.6",
    "styled-components": "^6.1.15",
    "swagger-ui-react": "^5.18.2",
    "tabulator-tables": "^5.6.1",
    "uuid": "^9.0.1",
