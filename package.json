{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.1.4",
  "type": "module",
  "proxy": "http://localhost:3001",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "start": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build",
    "lint": "eslint . --config src/config/eslint.config.js",
    "preview": "cross-env NODE_ENV=production vite preview",
    "swagger": "tsx src/services/swagger/generate.ts",
    "test": "cross-env NODE_ENV=test vitest",
    "test:integration": "cross-env NODE_ENV=test vitest integration --config src/config/vitest.config.ts",
    "test:unit": "cross-env NODE_ENV=test vitest unit --config src/config/vitest.config.ts",
    "test:coverage": "cross-env NODE_ENV=test vitest run --coverage",
    "test:setup": "cross-env NODE_ENV=test vitest --config src/config/vitest.config.ts tests/integration/setup/globalSetup.ts",
    "test:watch": "cross-env NODE_ENV=test vitest watch",
    "test:ui": "cross-env NODE_ENV=test vitest --ui",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prisma:merge": "node prisma/mergePrismaSchema.cjs",
    "prisma:migrate:dev": "cross-env NODE_ENV=development node scripts/prisma-migrate.js dev",
    "prisma:migrate:prod": "cross-env NODE_ENV=production node scripts/prisma-migrate.js deploy",
    "prisma:generate": "npm run prisma:merge && node scripts/prisma-generate.js",
    "prisma:push": "node scripts/prisma-db-push.js",
    "prisma:reset": "node scripts/prisma-reset.js",
    "prisma:deploy:dev": "cross-env NODE_ENV=development node scripts/deploy-prisma.js",
    "prisma:deploy:prod": "cross-env NODE_ENV=production node scripts/deploy-prisma.js",
    "prisma:deploy:seed": "node scripts/deploy-prisma.js --seed",
    "prisma:studio": "node scripts/prisma-studio.js",
    "db:test-connection": "node scripts/test-db-connection.js",
    "db:connect-supabase": "node scripts/connect-to-supabase.js",
    "db:setup-env": "node scripts/setup-database-env.js"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^6.4.0",
