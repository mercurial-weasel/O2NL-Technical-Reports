    "test:integration:tables": "vitest integration/airtable/table-operations --config vitest.config.ts",
    "test:unit": "vitest unit --config vitest.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:setup": "vitest --config vitest.config.ts tests/integration/setup/globalSetup.ts",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui"
